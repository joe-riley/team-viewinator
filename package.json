{
  "name": "team-viewinator",
  "version": "1.0.0",
  "description": "A quick and easy view into a team and a way to get their contact information.",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "lib": "lib"
  },
  "scripts": {
    "test": "jest",
    "dev": "./node_modules/nodemon/bin/nodemon.js index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joe-riley/team-viewinator.git"
  },
  "keywords": [
    "'Bootcamp'",
    "Silly Video Requirements",
    "Week 10",
    "Are you down with OOP?"
  ],
  "author": "Joe Riley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joe-riley/team-viewinator/issues"
  },
  "homepage": "https://github.com/joe-riley/team-viewinator#readme",
  "dependencies": {
    "express": "^4.17.1",
    "express-handlebars": "^5.2.0",
    "handlebars": "^4.7.6",
    "nodemon": "^2.0.6"
  },
  "devDependencies": {
    "eslint-config-react-app": "^6.0.0",
    "jest": "^26.6.3",
    "prettier-eslint": "^12.0.0"
  },
  "prettier": {
    "arrowParens": "avoid",
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "eqeqeq": "error",
      "no-console": "warn",
      "no-debugger": "warn",
      "no-var": "error",
      "prefer-const": "error",
      "no-shadow": "error"
    }
  }
}
